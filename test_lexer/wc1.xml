<cookcc>
	<lexer>
		<shortcut name="ws">[ \t]</shortcut>
		<shortcut name="nonws">[^ \t\n]</shortcut>
		<rule>
			<pattern>{nonws}+</pattern>
			<action>cc += yyleng; ++wc;</action>
		</rule>
		<rule>
			<pattern>{ws}+</pattern>
			<action>cc += yyleng;</action>
		</rule>
		<rule>
			<pattern>\n+</pattern>
			<action>++lc; ++cc;</action>
		</rule>
		<rule>
			<pattern><![CDATA[<<EOF>>]]></pattern>
			<action></action>
		</rule>
	</lexer>
</cookcc>
