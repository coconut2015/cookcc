<?xml version = "1.0" encoding="UTF-8"?>
<!DOCTYPE cookcc PUBLIC "-//CookCC//1.0" "http://code.google.com/p/cookcc/source/browse/trunk/src/resources/cookcc.dtd">
<cookcc>
	<!-- this example is intended to generate a warning of incomplete states from CookCC. -->
	<lexer table="ecs">
		<rule>
			<pattern>a</pattern>
			<action>System.out.print ("*" + getText () + "*");</action>
		</rule>
		<rule>
			<pattern>b</pattern>
			<action>System.out.print ("*" + getText () + "*");</action>
		</rule>
		<rule>
			<pattern><![CDATA[<<EOF>>]]></pattern>
			<action>System.out.println ("[[EOF]]"); return 0;</action>
		</rule>
	</lexer>
</cookcc>
