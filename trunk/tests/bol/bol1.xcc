<?xml version = "1.0" encoding="UTF-8"?>
<!DOCTYPE cookcc PUBLIC "-//CookCC//1.0" "http://code.google.com/p/cookcc/source/browse/trunk/src/resources/cookcc.dtd">
<cookcc>
	<lexer>
		<rule>
			<pattern>^abc</pattern>
			<action>System.out.println ("*" + getText () + " " + isBOL () + "*");</action>
		</rule>
		<rule>
			<pattern>abc</pattern>
			<pattern>.|\n</pattern>
			<action>System.out.println ("*" + getText () + "*");</action>
		</rule>
		<rule>
			<pattern><![CDATA[<<EOF>>]]></pattern>
			<action>return 0;</action>
		</rule>
	</lexer>
</cookcc>
