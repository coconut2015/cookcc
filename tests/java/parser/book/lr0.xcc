<?xml version = "1.0" encoding="UTF-8"?>
<!DOCTYPE cookcc PUBLIC "-//CookCC//1.0" "https://raw.githubusercontent.com/coconut2015/cookcc/master/src/resources/cookcc.dtd">
<cookcc>
	<lexer>
		<rule>
			<pattern>"("</pattern>
			<pattern>")"</pattern>
			<action>return yyText ().charAt (0);</action>
		</rule>
		<rule>
			<pattern>.|\n</pattern>
			<action>/* do nothing */</action>
		</rule>
		<rule>
			<pattern><![CDATA[<<EOF>>]]></pattern>
			<action>return 0;</action>
		</rule>
	</lexer>
	<parser start="S">
		<grammar rule="S">
			<rhs>'(' S ')' S</rhs>
			<action>
				_yyValue = "(" + yyGetValue (2) + ")" + yyGetValue (4);
				System.out.println (_yyValue);
			</action>
			<rhs></rhs>
			<action>
				_yyValue = "e";
				System.out.println ('e');
			</action>
		</grammar>
	</parser>
</cookcc>
