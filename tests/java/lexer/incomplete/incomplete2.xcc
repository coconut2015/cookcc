<?xml version = "1.0" encoding="UTF-8"?>
<!DOCTYPE cookcc PUBLIC "-//CookCC//1.0" "http://code.google.com/p/cookcc/source/browse/trunk/src/resources/cookcc.dtd">
<cookcc>
	<!-- This example shows that <<EOF>> token is also required for any states. -->
	<lexer table="ecs">
		<rule>
			<pattern>.|\n</pattern>
			<action>echo ();</action>
		</rule>
	</lexer>
</cookcc>
